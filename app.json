{
  "expo": {
    "name": "contacts-app",
    "slug": "gardon-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mine/phoneIcon.png",
    "scheme": "contacts-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wowmeww.gardonapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mine/phoneIcon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.wowmeww.gardonapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/mine/phoneIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mine/phoneIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6a660d71-dd38-4ae1-9f86-afe1ad8b7ce1"
      }
    }
  }
}
